  "updated_at": "?.LastModificationDate",
  "summary": "?.ExternalDescription",
  "location": {
    "text": "?.Address",
    "lat": null,
    "lng": null,
    "fields": {
      "city": "?.AddressDetails?.City",
      "state": "?.AddressDetails?.State",
      "country": "?.AddressDetails?.Country",
      "postcode": "?.AddressDetails?.PostalCode"
