{
  "name": "?.title",
  "picture": "?.pictures >> [] | .[0]?.default || null",
  "reference": "?.reference",
  "created_at": "?.published_at",
  "location": {
    "lat": "?.address >> null | ?.position?.lat | $float",
    "lng": "?.address >> null | ?.position?.lon | $float",
    "text": "?.address >> '' | ?.formatted"
  },
  "sections": [
    "?? .description : .description | { name: 'description', title: 'description', description: . | $sub('<.*?>', '') | $sub('&nbsp;', ' ') | $sub('&amp;', '&') | $sub('&quot;', '\"') | $sub('&apos;', ''') | $sub('&lt;', '<') | $sub('&gt;', '>') | $sub('\\s+', ' ') }",
    "?? .profile : .profile | { name: 'profile', title: 'profile', description: . | $sub('<.*?>', '') | $sub('&nbsp;', ' ') | $sub('&amp;', '&') | $sub('&quot;', '\"') | $sub('&apos;', ''') | $sub('&lt;', '<') | $sub('&gt;', '>') | $sub('\\s+', ' ') }"
  ],
  "requirements": "?.profile | $sub('<.*?>', '') | $sub('&nbsp;', ' ') | $sub('&amp;', '&') | $sub('&quot;', '\"') | $sub('&apos;', ''') | $sub('&lt;', '<') | $sub('&gt;', '>') | $sub('\\s+', ' ')",
  "skills": [".skills >> [] | $map({ name: ., type: null, value: null })"],
  "tags": [
    "?? .salary.min : .salary.min | { name: 'digitalrecruiters_compensation_min', value: . }",
    "?? .salary.max : .salary.max | { name: 'digitalrecruiters_compensation_max', value: . }",
    "?? .salary.currency : .salary.currency | { name: 'digitalrecruiters_compensation_currency', value: . }",
    "?? .entity.manager.firstname : .entity.manager.firstname | { name: 'digitalrecruiters_manager_firstName', value: . }",
    "?? .entity.manager.lastname : .entity.manager.lastname | { name: 'digitalrecruiters_manager_lastName', value: . }",
    "?? .entity.manager.position : .entity.manager.position | { name: 'digitalrecruiters_manager_position', value: . }",
    "?? .entity.manager.picture_url : .entity.manager.picture_url | { name: 'digitalrecruiters_manager_picture', value: . }",
    "?? .referent_recruiter.email : .referent_recruiter.email | { name: 'digitalrecruiters_recruiter_email', value: . }",
    "?? .referent_recruiter.phoneNumber : .referent_recruiter.phoneNumber | { name: 'digitalrecruiters_recruiter_phoneNumber', value: . }",
    "?? .referent_recruiter.picture_url : .referent_recruiter.picture_url | { name: 'digitalrecruiters_recruiter_picture', value: . }",
    "?? .hierarchy : .hierarchy | $map({ name: ?.depth | $concat('hierarchy_', .), value: . | $concat(?.column_name, ':', ?.public_name) })"
  ]
}
