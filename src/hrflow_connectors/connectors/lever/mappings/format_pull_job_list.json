{
  "reference": ".id",
  "name": ".text",
  "location": {
    "text": "?.categories?.location >> null",
    "lat": null,
    "lng": null
  },
  "url": ".urls.show",
  "summary": ".content.description",
  "sections": [
    "?? .content.lists.[0] | .content.lists | $map({ name: .text, title: .text, description: null })"
  ],
  "tags": [
    {
      "name": "lever_confidential",
      "value": null
    },
    ".categories || {} | $items | $map({ name: . | $concat(lever_, .key ), value: .value })",
    ".tags || [] | $map({ name: . | $concat(lever_additional_tag_, .__idx | $string), value: . })",
    ".distributionChannels || [] | $map({ name: . | $concat(lever_distributionChannel_, .__idx | $string), value: . })",
    ".urls || {} | $items | $map({ name: . | $concat(lever_url_, .key), value: .value })",
    "?? .country : .country | { name: lever_country, value: . }",
    "?? .state : .state | { name: lever_state, value: . }",
    "?? .workplaceType : .workplaceType | { name: lever_workplaceType, value: . }"
  ],
  "ranges_float": [
    "?? .salaryRange : .salaryRange | { name: salary, value_min: ?.min, value_max: ?.max, unit: ?.currency }"
  ],
  "created_at": null,
  "updated_at": null
}
